<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.demo.physiology_info.mapper.NlEmployeePhysiologyInfoMapper">

    <select id="getByEmployeeId"
            resultType="org.jeecg.modules.demo.physiology_info.entity.NlEmployeePhysiologyInfo">
        select *
        from nl_employee_physiology_info
        where employee_id = #{employeeId}
    </select>
</mapper>