<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.demo.nl_employee_assess_management.nl_employee_info.mapper.NlEmployeeInfoMapper">
    <select id="getInfoByEmployeeIdWithName"
            resultType="org.jeecg.modules.demo.nl_employee_assess_management.nl_employee_info.vo.EmployeeInfoVO">
        select info.*, user.realname
        from nl_employee_info info,
             sys_user user
        where info.employee_id = user.id
    </select>
</mapper>